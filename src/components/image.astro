---
const backgrounds = [
  "https://ik.imagekit.io/danielcmadeley/madeleydesignstudio/background-1.png?updatedAt=1739058183985",
  "https://ik.imagekit.io/danielcmadeley/madeleydesignstudio/background-2.png?updatedAt=1739058203974",
  "https://ik.imagekit.io/danielcmadeley/madeleydesignstudio/background-3.png?updatedAt=1739058220606",
];
const randomBackground =
  backgrounds[Math.floor(Math.random() * backgrounds.length)];
---

<img
  src={randomBackground}
  alt="Background"
  class="fixed inset-0 w-full h-full object-cover -z-10"
/>
